{
    "name": "MkDocs",
    "id": "mkdocs",
    "version": "1.0.0",
    "description": "MkDocs is a fast, simple and downright gorgeous static site generator that's geared towards building project documentation.",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/mkdocs",
    "installAfter": [
        "ghcr.io/devcontainers/features/python:latest"
    ],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the MKDocs version you would like to install"
        },
        "includePluginMkdocsmaterial": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin mkdocs-material"
        },
        "pluginMkdocsmaterialVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin mkdocs-material version you would like to install"
        },
        "includePluginPymdownextensions": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin pymdown-extensions"
        },
        "pluginPymdownextensionsVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin pymdown-extensions version you would like to install"
        },
        "includePluginMkdocstrings": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin mkdocstrings"
        },
        "pluginMkdocstringsVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin mkdocstrings version you would like to install"
        },
        "includePluginMkdocsmonorepoplugin": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin mkdocs-monorepo-plugin"
        },
        "pluginMkdocsmonorepopluginVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin mkdocs-monorepo-plugin version you would like to install"
        },
        "includePluginMkdocspdfexportplugin": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin mkdocs-pdf-export-plugin"
        },
        "pluginMkdocspdfexportpluginVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin mkdocs-pdf-export-plugin version you would like to install"
        },
        "includePluginMkdocsawesomepagesplugin": {
            "type": "boolean",
            "default": true,
            "description": "Include plugin mkdocs-awesome-pages-plugin"
        },
        "pluginMkdocsawesomepagespluginVersion": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the plugin mkdocs-awesome-pages-plugin version you would like to install"
        }
    }
}